package com.hcl.obj;

public class Array {
int empno;
String name;
double basic;
	public Array[] showEmploy(){
		Array[] arrEmploy=new Array[3];
		arrEmploy[0] arrEmploy=new Array();
		arrEmploy[0].empno=1;
		arrEmploy[0].name="vinod";
		arrEmploy[0].basic=6666;
		arrEmploy[1] arrEmploy=new Array();
		arrEmploy[1].empno=12;
		arrEmploy[1].name="vinodd";
		arrEmploy[1].basic=66667;
		arrEmploy[2] arrEmploy=new Array();
		arrEmploy[2].empno=16;
		arrEmploy[2].name="vinodfdd";
		arrEmploy[2].basic=6666767;
		return arrEmploy;
	}
	public static void main(String[] args){
		Array[] result=new Array().showEmploy();
		for (Employ e : result) {
			
			System.out.println(e.empno+""+e.name""+e.basic);
			
		}
		
	}

}
