package com.hcl.ex;
public class Div {
	public static void main(String[] args) {
		int a=2,b=4,c;
     	try {
			a=Integer.parseInt(args[0]);
			b=Integer.parseInt(args[1]);
			c=a/b;
			System.out.println(c);
		} catch (Exception e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
		
		
		
	
		}
		
	}
