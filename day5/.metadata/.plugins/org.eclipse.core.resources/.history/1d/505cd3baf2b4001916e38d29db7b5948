package com.hcl.collections;

import java.util.ArrayList;
import java.util.List;

public class ArrayListDemo {
	public static void main(String[] args) {
	List lstNames=new ArrayList();
	lstNames.add("anubhav");
	lstNames.add("vinod");
	lstNames.add("prem");
	lstNames.add("raghu");
	lstNames.add("sai");
	lstNames.add("kumar");
	
	}

}
