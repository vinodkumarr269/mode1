package com.hcl.ass;
/**
 * main is started.
 * @author Coalesce from vinod.
 */

public class AssertDemo {
  public void incr() { 
		int basic=30000;
		for(int i=0;i<8;i++){
			basic =basic+4000;
			assert basic<48000;
			System.out.println(basic);
		}
	}
	public static void main(String[] args) {
		new AssertDemo().incr();
	}

}
